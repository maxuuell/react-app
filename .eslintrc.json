{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "react/jsx-indent": ["error", "tab"],
        "react/no-unescaped-entities": 0,
        "jsx-a11y/anchor-is-valid": 0
      }
}